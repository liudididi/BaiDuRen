<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.login_demo">

    <!-- 往sdcard中写入数据的权限 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="design_width"
            android:value="1080" />
        <meta-data
            android:name="design_height"
            android:value="1920" />

        <activity android:name=".MainActivity" />
        <activity
            android:name=".HomeActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GetBackPassActivity" />
        <activity android:name=".SetTingActivity" />
        <activity android:name=".MySchoolActivity" />
        <activity android:name=".PresonMessageActivity" />
        <activity android:name=".CharacterActivity" />
        <activity android:name=".ParticularsActivity" />
        <activity android:name=".SearchParticularsActivity" />
        <activity android:name=".UserAgreenActivity" />
        <activity android:name=".AccountMagActivity" />
        <activity android:name=".ChangPhoneActivity"></activity>

        <activity android:name=".AddServeActivity" />
        <activity android:name=".SuggestActivity" />
        <activity android:name=".HelpActivity"></activity>
    </application>

</manifest>