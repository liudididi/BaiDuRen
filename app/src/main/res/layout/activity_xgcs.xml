<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.login_demo.XGcsActivity">
    <RelativeLayout
        android:background="#fff"
        android:id="@+id/xqguihua_title"
        android:layout_width="match_parent"
        android:layout_height="120px"
        >
        <ImageView
            android:padding="10px"
            android:id="@+id/xgguihua_iv_back"
            android:layout_width="150px"
            android:layout_height="90px"
            android:layout_gravity="center"
            android:src="@mipmap/left"
            android:layout_centerVertical="true"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="EFC学业规划报告"
            android:textColor="#2c2c2c"
            android:textSize="50px" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="#ccc"
            android:layout_alignParentBottom="true"
            ></View>
    </RelativeLayout>

    <RelativeLayout
        android:background="#F3F3F3"
        android:layout_below="@+id/xqguihua_title"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <RelativeLayout
                    android:id="@+id/xqrl_yi"
                    android:background="#fff"
                    android:layout_marginLeft="40px"
                    android:layout_marginRight="40px"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_marginTop="30px"
                        android:layout_centerHorizontal="true"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="性格测试报告"
                        android:textStyle="bold"
                        android:textSize="60px"
                        />

                    <ImageView
                        android:src="@drawable/next"
                        android:layout_marginTop="30px"
                        android:padding="30px"
                        android:scaleType="centerCrop"
                        android:layout_marginRight="30px"
                        android:layout_alignParentRight="true"
                        android:layout_width="90px"
                        android:layout_height="90px"

                        />
                    <TextView
                        android:layout_marginLeft="360px"
                        android:textSize="50px"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="聪颖机制解决问题的人"
                        android:layout_marginTop="160px"
                        />
                    <TextView
                        android:layout_marginLeft="100px"
                        android:textColor="#F00"
                        android:text="INTP"
                        android:textSize="50px"
                        android:gravity="center"
                        android:layout_marginTop="160px"
                        android:layout_width="200px"
                        android:layout_height="match_parent" />
                    <View

                        android:layout_width="match_parent"
                        android:layout_marginTop="240px"
                        android:layout_height="20px"></View>
                </RelativeLayout>


                <LinearLayout
                    android:orientation="vertical"
                    android:layout_marginLeft="40px"
                    android:layout_marginRight="40px"
                    android:background="#fff"
                    android:layout_width="match_parent"
                    android:layout_height="500px">
<LinearLayout
    android:orientation="horizontal"
    android:gravity="center_vertical"
    android:layout_marginTop="50px"
    android:layout_width="match_parent"
    android:layout_height="100px">
    <TextView
        android:layout_marginLeft="30px"
        android:text="外向"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:layout_marginLeft="10px"
        android:text="(E)"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
     <ProgressBar
        android:id="@+id/pb1"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="270px"
        android:layout_height="20px"
        android:max="100"
        android:progress="100"
         android:layout_marginLeft="10px"
        android:progressDrawable="@drawable/progressbg"/>
    <ProgressBar
        android:id="@+id/pb2"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="270px"
        android:layout_height="20px"
        android:max="100"
        android:progress="35"
        android:layout_marginLeft="30px"
        android:progressDrawable="@drawable/progressbg1"/>

    <TextView
        android:layout_marginLeft="30px"
        android:text="(I)"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:layout_marginLeft="10px"
        android:text="内向"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</LinearLayout>

                    <LinearLayout
                        android:orientation="horizontal"
                        android:gravity="center_vertical"
                        android:layout_width="match_parent"
                        android:layout_height="100px">
                        <TextView
                            android:layout_marginLeft="30px"
                            android:text="感觉"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                        <TextView
                            android:layout_marginLeft="10px"
                            android:text="(S)"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                        <ProgressBar
                            android:id="@+id/pb3"
                            style="?android:attr/progressBarStyleHorizontal"
                            android:layout_width="270px"
                            android:layout_height="20px"
                            android:max="100"
                            android:progress="100"
                            android:layout_marginLeft="10px"
                            android:progressDrawable="@drawable/progressbg2"/>
                        <ProgressBar
                            android:id="@+id/pb4"
                            style="?android:attr/progressBarStyleHorizontal"
                            android:layout_width="270px"
                            android:layout_height="20px"
                            android:max="100"
                            android:progress="45"
                            android:layout_marginLeft="30px"
                            android:progressDrawable="@drawable/progressbg3"/>

                        <TextView
                            android:layout_marginLeft="30px"
                            android:text="(N)"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                        <TextView
                            android:layout_marginLeft="10px"
                            android:text="直觉"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                    </LinearLayout>


                    <LinearLayout
                        android:orientation="horizontal"
                        android:gravity="center_vertical"
                        android:layout_width="match_parent"
                        android:layout_height="100px">
                        <TextView
                            android:layout_marginLeft="30px"
                            android:text="思考"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                        <TextView
                            android:layout_marginLeft="10px"
                            android:text="(T)"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                        <ProgressBar
                            android:id="@+id/pb5"
                            style="?android:attr/progressBarStyleHorizontal"
                            android:layout_width="270px"
                            android:layout_height="20px"
                            android:max="100"
                            android:progress="100"
                            android:layout_marginLeft="10px"
                            android:progressDrawable="@drawable/progressbg4"/>
                        <ProgressBar
                            android:id="@+id/pb6"
                            style="?android:attr/progressBarStyleHorizontal"
                            android:layout_width="270px"
                            android:layout_height="20px"
                            android:max="100"
                            android:progress="55"
                            android:layout_marginLeft="30px"
                            android:progressDrawable="@drawable/progressbg5"/>

                        <TextView
                            android:layout_marginLeft="30px"
                            android:text="(F)"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                        <TextView
                            android:layout_marginLeft="10px"
                            android:text="情感"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                    </LinearLayout>


                    <LinearLayout
                        android:orientation="horizontal"
                        android:gravity="center_vertical"
                        android:layout_width="match_parent"
                        android:layout_height="100px">
                        <TextView
                            android:layout_marginLeft="30px"
                            android:text="判断"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                        <TextView
                            android:layout_marginLeft="10px"
                            android:text="(J)"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                        <ProgressBar
                            android:id="@+id/pb7"
                            style="?android:attr/progressBarStyleHorizontal"
                            android:layout_width="270px"
                            android:layout_height="20px"
                            android:max="100"
                            android:progress="100"
                            android:layout_marginLeft="10px"
                            android:progressDrawable="@drawable/progressbg6"/>
                        <ProgressBar
                            android:id="@+id/pb8"
                            style="?android:attr/progressBarStyleHorizontal"
                            android:layout_width="270px"
                            android:layout_height="20px"
                            android:max="100"
                            android:progress="65"
                            android:layout_marginLeft="30px"
                            android:progressDrawable="@drawable/progressbg7"/>

                        <TextView
                            android:layout_marginLeft="30px"
                            android:text="(P)"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                        <TextView
                            android:layout_marginLeft="10px"
                            android:text="直觉"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                    </LinearLayout>


                </LinearLayout>
 <ImageView
     android:src="@drawable/xgcsbg"
     android:layout_marginTop="30px"
     android:layout_marginLeft="40px"
     android:layout_marginRight="40px"
     android:layout_width="match_parent"
     android:layout_height="wrap_content">
 </ImageView>

            </LinearLayout>


        </ScrollView>
    </RelativeLayout>
</RelativeLayout>
